{
  "name": "meetups-clone",
  "version": "1.0.0",
  "description": "Meetups Clone",
  "main": "server.js",
  "repository": "https://github.com/collinstheuncoder/meetups-clone.git",
  "author": "Collins Orlando",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "nodemon --exec babel-node server/src/server.js",
    "client": "cd client && yarn serve",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "prebuild": "rm -rf server/dist",
    "build-babel": "babel -d ./server/dist ./server/src -s",
    "build": "yarn build-babel",
    "start": "node server/build/server.js",
    "postinstall": "cd client && yarn build"
  },
  "dependencies": {
    "apollo-server-express": "1.3.4",
    "bcrypt": "2.0.1",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-tools": "3.0.4",
    "jsonwebtoken": "^8.3.0",
    "merge-graphql-schemas": "1.5.1",
    "mongoose": "^5.3.1",
    "mongoose-validator": "^2.1.0",
    "morgan": "^1.9.1",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.4"
  },
  "engines": {
    "node": "10.11.0",
    "yarn": "1.10.1"
  }
}
